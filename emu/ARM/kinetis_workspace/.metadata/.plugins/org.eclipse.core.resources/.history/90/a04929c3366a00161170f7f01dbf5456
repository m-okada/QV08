#ifndef SOURCES_SDRAM_PIN_H_
#define SOURCES_SDRAM_PIN_H_

enum _SD_Command{
	MRS=0,
	REF,
	SELF,
	END_SELF,
	PRE,	//	Precharge
	PALL,	//	Precharge ALL.
	ACT,	//	Bank Activation.
} ;



#endif /* SOURCES_SDRAM_PIN_H_ */
